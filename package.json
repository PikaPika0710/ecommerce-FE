{
    "name": "e-commerce-website",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "watch": "vue-cli-service build --watch",
        "serve": "vue-cli-service serve",
        "build": "vue-cli-service build",
        "lint": "vue-cli-service lint"
    },
    "watch": {
        "build": {
            "patterns": [
                "src"
            ],
            "extensions": "js,jsx"
        }
    },
    "dependencies": {
        "axios": "^0.27.2",
        "bulma": "^0.9.4",
        "bulma-toast": "^2.4.1",
        "core-js": "^3.8.3",
        "font-awesome": "^4.7.0",
        "npm-watch": "^0.11.0",
        "sas": "^3.0.4",
        "sass": "^1.53.0",
        "sass-loader": "^13.0.0",
        "vue": "^3.2.13",
        "vue-router": "^4.0.16",
        "vuex": "^4.0.2"
    },
    "engines": {
        "node": "16.x"
    },
    "devDependencies": {
        "@babel/core": "^7.12.16",
        "@babel/eslint-parser": "^7.12.16",
        "@vue/cli-plugin-babel": "~5.0.0",
        "@vue/cli-plugin-eslint": "~5.0.0",
        "@vue/cli-service": "~5.0.0",
        "bootstrap": "^4.6.1",
        "eslint": "^7.32.0",
        "eslint-plugin-vue": "^8.0.3",
        "jquery": "^3.6.0",
        "popper.js": "^1.16.1",
        "webpack-cli": "^4.10.0"
    },
    "eslintConfig": {
        "root": true,
        "env": {
            "node": true
        },
        "extends": [
            "plugin:vue/vue3-essential",
            "eslint:recommended"
        ],
        "parserOptions": {
            "parser": "@babel/eslint-parser"
        },
        "rules": {}
    },
    "browserslist": [
        "> 1%",
        "last 2 versions",
        "not dead",
        "not ie 11"
    ],
    "overrides": {
        "autoprefixer": "10.4.5"
    }
}